<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.image.ImageView?>
<?import javafx.scene.image.Image?>
<Pane fx:id="root" xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="client.view.game.prematch.ClientQuickMatchMenu"
            prefHeight="400.0" prefWidth="600.0">
    <children>
        <ImageView fitHeight="768.0" fitWidth="1366.0" pickOnBounds="true">
            <Image url="@../images/backgrounds/MatchFinderMenu.jpg"/>
        </ImageView>
        <ListView fx:id="matches" layoutX="533.0" layoutY="90.0" prefHeight="200.0" prefWidth="300.0" stylesheets="@../CSS/listviewstyle.css" />
        <Button id="ipad-dark-grey" layoutX="608.0" layoutY="315.0" mnemonicParsing="false" onMouseClicked="#start" style="-fx-font-family: 'Mason Serif Regular'; -fx-font-size: 18;" stylesheets="@../CSS/buttonstyle.css, @../CSS/textstyle.css" text="Start" />
        <Button id="ipad-dark-grey" layoutX="608.0" layoutY="380.0" mnemonicParsing="false" onMouseClicked="#createNewMatch" style="-fx-font-family: 'Mason Serif Regular'; -fx-font-size: 18;" stylesheets="@../CSS/buttonstyle.css, @../CSS/textstyle.css" text="New Match"/>
        <Button opacity="0" fx:id="cancel" id="ipad-dark-grey" layoutX="608.0" layoutY="615.0" mnemonicParsing="false" onMouseClicked="#cancel" style="-fx-font-family: 'Mason Serif Regular'; -fx-font-size: 18;" stylesheets="@../CSS/buttonstyle.css, @../CSS/textstyle.css" text="cancel"/>
        <Button id="ipad-dark-grey" layoutX="608.0" layoutY="680.0" mnemonicParsing="false" onMouseClicked="#back" style="-fx-font-family: 'Mason Serif Regular'; -fx-font-size: 18;" stylesheets="@../CSS/buttonstyle.css, @../CSS/textstyle.css" text="back" />
    </children>
</Pane>
